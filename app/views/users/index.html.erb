<header id="content-header">
  <h4><%= pluralize(@users.size, 'User') %></h4>
</header>

<%= link_to "download", users_path(format: :csv), class: "btn btn-secondary" %>
<br><br>
<ul id="users">
  <% @users.each do |user| %>
  <li>
    <%= link_to user.email, user %>
  </li>
  <% end %>
</ul>

<br>

